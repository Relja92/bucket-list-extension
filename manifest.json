{
  "manifest_version": 3,
  "name": "Bucket List Extension",
  "version": "1.0",
  "permissions": ["contextMenus", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "64": "logo.png"
  }
}
